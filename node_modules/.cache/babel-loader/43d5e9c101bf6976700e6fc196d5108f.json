{"ast":null,"code":"import vTableRow from './v-table-row';\nexport default {\n  name: \"v-table\",\n  components: {\n    vTableRow\n  },\n  props: {\n    users_data: {\n      type: Array,\n      default: () => {\n        return [];\n      }\n    }\n  },\n\n  data() {\n    return {\n      usersPerPage: 5,\n      pageNumber: 1,\n      searchData: \"\"\n    };\n  },\n\n  computed: {\n    pages() {\n      return Math.ceil(this.users_data.length / 5);\n    },\n\n    paginatedUsers() {\n      let from = (this.pageNumber - 1) * this.usersPerPage;\n      let to = from + this.usersPerPage;\n      return this.users_data.slice(from, to);\n    },\n\n    filteredProduct() {\n      let filtered = this.filteredData.filter(function (item) {\n        return item.name.indexOf(this.searchData) > -1;\n      });\n      return filtered;\n    }\n\n  },\n  methods: {\n    pageClick(page) {\n      this.pageNumber = page;\n    } // sortByName(){\n    //   this.users_data.sort((a, b) => a.name.localeCompare(b.name))\n    // this.users_data.sort((a,b) => a.name.localeCompare(b.name))\n    // },\n    // sortByAmount() {\n    //   this.users_data.sort((a,b) => a.amount - b.amount)\n    // }\n    //   sortByDistance(){\n    //   this.users_data.sort((a,b) => a.distance - b.distance)\n    // }\n\n\n  }\n};","map":{"version":3,"mappings":"AA8BA,OAAOA,SAAP,MAAsB,eAAtB;AAEA,eAAe;EACbC,IAAI,EAAE,SADO;EAEbC,UAAU,EAAE;IACVF;EADU,CAFC;EAKbG,KAAK,EAAE;IACLC,UAAU,EAAE;MACVC,IAAI,EAAEC,KADI;MAEVC,OAAO,EAAE,MAAM;QACb,OAAO,EAAP;MACD;IAJS;EADP,CALM;;EAabC,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,CADT;MAELC,UAAU,EAAE,CAFP;MAGLC,UAAU,EAAE;IAHP,CAAP;EAKD,CAnBY;;EAoBbC,QAAQ,EAAE;IACRC,KAAK,GAAE;MACL,OAAOC,IAAI,CAACC,IAAL,CAAU,KAAKX,UAAL,CAAgBY,MAAhB,GAAyB,CAAnC,CAAP;IACD,CAHO;;IAIRC,cAAc,GAAE;MACd,IAAIC,IAAG,GAAI,CAAC,KAAKR,UAAL,GAAiB,CAAlB,IAAuB,KAAKD,YAAvC;MACA,IAAIU,EAAC,GAAID,IAAG,GAAI,KAAKT,YAArB;MACA,OAAO,KAAKL,UAAL,CAAgBgB,KAAhB,CAAsBF,IAAtB,EAA4BC,EAA5B,CAAP;IACD,CARO;;IASRE,eAAe,GAAE;MACf,IAAIC,QAAO,GAAI,KAAKC