{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions, mapGetters } from 'vuex';\nimport vTable from './components/table/v-table';\nimport vSelect from './components/v-select';\nexport default {\n  name: 'app',\n  components: {\n    vTable,\n    vSelect\n  },\n  data: () => {\n    return {\n      categories: [{\n        name: 'Фрукты',\n        value: 1\n      }, {\n        name: 'Овощи',\n        value: 2\n      }, {\n        name: 'Все продукты',\n        value: 3\n      }],\n      selected: \"Выбрать категории:\",\n      sortedData: [],\n      searchData: \"\"\n    };\n  },\n  computed: { ...mapGetters(['USERS']),\n\n    filteredData() {\n      if (this.sortedData.length) {\n        return this.sortedData;\n      } else {\n        return this.USERS;\n      }\n    },\n\n    filteredProduct() {\n      let filtered = this.filteredData.filter(function (item) {\n        return item.name.indexOf(this.searchData) > -1;\n      });\n      return filtered;\n    }\n\n  },\n  methods: {\n    optionSelect(category) {\n      this.sortedData = [];\n      let vm = this;\n      this.USERS.map(function (item) {\n        if (item.category === category.name) {\n          vm.sortedData.push(item);\n        }\n      }), this.selected = category.name;\n    },\n\n    ...mapActions(['GET_USERS_FROM_API'])\n  },\n\n  mounted() {\n    this.GET_USERS_FROM_API();\n  }\n\n};","map":{"version":3,"mappings":";AAkBA,SAAQA,UAAR,EAAoBC,UAApB,QAAqC,MAArC;AACA,OAAOC,MAAP,MAAmB,4BAAnB;AACA,OAAOC,OAAP,MAAoB,uBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IACVH,MADU;IAEVC;EAFU,CAFC;EAObG,IAAI,EAAE,MAAM;IACV,OAAM;MACJC,UAAU,EAAE,CACV;QAACH,IAAI,EAAE,QAAP;QAAiBI,KAAK,EAAE;MAAxB,CADU,EAEV;QAACJ,IAAI,EAAE,OAAP;QAAgBI,KAAK,EAAE;MAAvB,CAFU,EAGV;QAACJ,IAAI,EAAE,cAAP;QAAuBI,KAAK,EAAE;MAA9B,CAHU,CADR;MAMJC,QAAQ,EAAE,oBANN;MAOJC,UAAU,EAAE,EAPR;MAQJC,UAAU,EAAE;IARR,CAAN;EAUD,CAlBY;EAmBbC,QAAQ,EAAE,EACR,GAAGX,UAAU,CAAC,CACZ,OADY,CAAD,CADL;;IAIRY,YAAY,GAAE;MACZ,IAAG,KAAKH,UAAL,